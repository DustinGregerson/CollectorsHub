@model RegisterViewModel
<form asp-action="Register">
    <div class="form-group">
    <label for="FirstName">First Name:</label>
    <input class="form-control" asp-for="FirstName" type="text">
    </div>
    <div class="form-group">
    <label for="LastName">Last Name:</label>
    <input class="form-control" asp-for="LastName" type="text">
    </div>
    <div class="form-group">
    <label for="UserName">User Name:</label>
    <input class="form-control" asp-for="UserName" type="text">
    </div>
    <div class="form-group">
    <label for="Password">Password:</label>
    <input class="form-control" asp-for="Password" type="text">
    </div>
    <button class="btn btn-primary" type="submit">Submit</button>
</form>
